 		jjtThis.setQuote(quote);
 		jjtThis.setUsingRowDelimiter(useRowDelimiter);
 		jjtThis.setSelector(selector);
 		if (noTrim) {
            jjtThis.setNoTrim();
        }
        jjtThis.setRowDelimiter(rowDelimiter);
 		return jjtThis;
 	}
