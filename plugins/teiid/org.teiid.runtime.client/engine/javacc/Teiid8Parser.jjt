SPECIAL_TOKEN:
{
    <LINE_COMMENT: "--"(~["\r","\n"])*> {

        if (parser.versionLessThan(Version.TEIID_8_10))
            return;

        // Calculates the offset for these special tokens
        calculateOffset(matchedToken);

        createComment(false, matchedToken);

        trackToken(matchedToken);    }
}

