<plugin>

   <extension
         point="org.teiid.designer.extension.modelExtensionProvider">
      <extensionDefinition
            className="org.teiid.designer.runtime.extension.rest.RestModelExtensionAssistant"
            path="rest.mxd">
      </extensionDefinition>
   </extension>
   <extension
         point="org.teiid.designer.extension.modelExtensionProvider">
      <extensionDefinition
            className="org.teiid.designer.runtime.extension.odata.ODataModelExtensionAssistant"
            path="odata.mxd">
      </extensionDefinition>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.wst.server.core.IServer"
            class="org.teiid.designer.runtime.adapter.TeiidServerAdapterFactory">
         <adapter
               type="org.teiid.designer.runtime.spi.ITeiidServer">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.teiid.designer.runtime.DqpPluginStartup">
      </startup>
   </extension>
   <extension
         point="org.teiid.designer.spi.teiidServerManager">
      <serverManager
            class="org.teiid.designer.runtime.TeiidServerManager">
      </serverManager>
   </extension>

</plugin>
